@model Winterra.Models.ViewModels.ErrorViewModel

<div class="d-flex justify-content-center align-items-center w-100 h-100">
    <div class="position-relative">
        <img src="~/assets/img/error401.png" alt="Access Forbidden">
        <div class="error-body text-center">
            <span class="error-text">403</span>
            <span class="error-details">Access Denied – You don’t have the required permissions to view this page.</span>
            <button onclick="history.back()" class="primary-btn mt-5">Go Back</button>
        </div>
    </div>
</div>
